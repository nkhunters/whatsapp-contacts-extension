{
  "name": "Whatsapp Contacts",
  "description": "Save unsaved whatsapp contacts",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["declarativeContent", "activeTab", "tabs", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
